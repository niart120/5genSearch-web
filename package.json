{
  "name": "5gensearch-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm build:wasm && tsc -b && vite build",
    "build:wasm": "wasm-pack build wasm-pkg --target web --out-dir ../src/wasm --release -- --features gpu && node scripts/optimize-wasm.js && node scripts/copy-wasm.js",
    "build:wasm:dev": "wasm-pack build wasm-pkg --target web --out-dir ../src/wasm --dev -- --features gpu && node scripts/copy-wasm.js",
    "lint": "eslint . && cargo clippy -p wasm-pkg --all-targets -- -D warnings",
    "lint:ts": "eslint .",
    "format": "prettier --write . && cargo fmt",
    "format:check": "prettier --check . && cargo fmt --check",
    "format:check:ts": "prettier --check .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:wasm": "cargo test -p wasm-pkg",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile --typescript"
  },
  "dependencies": {
    "@fontsource-variable/noto-sans-jp": "^5.2.10",
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@lingui/core": "^5.9.0",
    "@lingui/react": "^5.9.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@lingui/babel-plugin-lingui-macro": "^5.9.0",
    "@lingui/cli": "^5.9.0",
    "@lingui/vite-plugin": "^5.9.0",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@webgpu/types": "^0.1.69",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "playwright": "^1.58.1",
    "prettier": "^3.8.0",
    "skills": "^1.3.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  }
}
